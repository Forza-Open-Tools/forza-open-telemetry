{
  "name": "forza-telemetry",
  "version": "0.0.1",
  "keywords": [
    "telemetry",
    "Forza",
    "Forza Horizon 5",
    "Forza Open Tunes"
  ],
  "license": "MIT",
  "author": {
    "name": "Aaron Throckmorton",
    "email": "aaron@throckmortons.com"
  },
  "repository": "https://github.com/Forza-Open-Tools/forza-open-telemetry",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron": "wait-on tcp:3333 && IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"BROWSER=none npm run dev\" \"npm run electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "vue-tsc --noEmit && ELECTRON=true vite build",
    "app:build": "yarn build:for:electron && yarn electron:builder"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "change-case": "^4.1.2",
    "chart.js": "^3.7.1",
    "dayjs": "^1.11.2",
    "electron-is-dev": "^2.0.0",
    "forza-open-telemetry-server": "file:../forza-open-telemetry-server",
    "gauge-chart": "^0.5.3",
    "has-hover": "^1.0.1",
    "lodash.debounce": "^4.0.8",
    "node-ipc": "^11.1.0",
    "socket.io-client": "^4.5.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.33",
    "vue-chart-3": "^3.1.8"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.7",
    "@types/node-ipc": "^9.2.0",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.0",
    "electron": "^18.2.3",
    "electron-builder": "^23.0.3",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.15",
    "wait-on": "^6.0.1"
  }
}
