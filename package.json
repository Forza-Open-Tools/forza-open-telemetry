{
  "name": "forza-open-telemetry",
  "version": "0.0.1",
  "keywords": [
    "telemetry",
    "Forza",
    "Forza Horizon 5",
    "Forza Open Tunes"
  ],
  "license": "MIT",
  "author": {
    "name": "Aaron Throckmorton",
    "email": "aaron@throckmortons.com"
  },
  "type": "module",
  "repository": "https://github.com/Forza-Open-Tools/forza-open-telemetry",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron": "wait-on tcp:3333 && IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"BROWSER=none npm run dev\" \"npm run electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "vue-tsc --noEmit && ELECTRON=true vite build",
    "app:build": "npm run build:for:electron && npm run electron:builder"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "canvas-gauges": "^2.1.7",
    "change-case": "^5.2.0",
    "chart.js": "^4.4.0",
    "dayjs": "^1.11.10",
    "electron-is-dev": "^2.0.0",
    "gauge-chart": "^1.0.0",
    "has-hover": "^1.0.1",
    "lodash.debounce": "^4.0.8",
    "node-ipc": "^11.1.0",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.1",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0"
  },
  "devDependencies": {
    "@babel/types": "^7.23.4",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node-ipc": "^9.2.3",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22",
    "wait-on": "^7.2.0"
  }
}
